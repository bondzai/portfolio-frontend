run = "sh -c 'cd ./backend && go run .'"
compile = "sh -c 'cd ./backend && go build .'" 

[languages.go]
pattern = "**/*.go"

[languages.go.languageServer]
start = "gopls"

[nix]
channel = "stable-22_11"

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[deployment]
run = ["sh", "-c", "cd ./backend && ./main"]
build = ["sh", "-c", "cd ./backend && go build ."]
